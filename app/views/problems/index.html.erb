<h1>Problems</h1>

<table class="table">
  <tr>
    <th>#</th>
    <th>Title</th>
    <th>Question's</th>
    <th>Published Date</th>
  </tr>
  <% @problems.each do |p| %>
  <tr>
    <td><%= p.id %></td>

    <td><%= link_to p.title, {action:"show", controller:"problems", id: p.id} %></td>
    <td><%= link_to p.user.email, {action:"show", controller:"users", id: p.user.id} %></td>
    <td><%= p.updated_at %></td>
  </tr>

  <% end %>
</table>
